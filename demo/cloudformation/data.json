{
  "AWSTemplateFormatVersion" : "2010-09-09",

  "Description" : "Networking Stack",

  "Metadata" : {
  },

  "Parameters" : {
    "Target" : {
      "Type" : "String"
    },
    "Stack" : {
      "Type" : "String"
    },
    "VPCCIDRBlock" : {
      "Type" : "String"
    },
    "PubSubCIDR1" : {
      "Type" : "String"
    },
    "PubSubCIDR2" : {
      "Type" : "String"
    },
    "PrivSubCIDR1" : {
      "Type" : "String"
    },
    "PrivSubCIDR2" : {
      "Type" : "String"
    }
  },

  "Mappings" : {
  },

  "Conditions" : {
  },

  "Resources" : {
    "VPC" : {
      "Type" : "AWS::EC2::VPC",
      "Properties" : {
        "CidrBlock" : { "Ref" : "VPCCIDRBlock" },
        "EnableDnsHostnames" : "true",
        "Tags" : [ { "Key" : "Name", "Value" : { "Fn::Join" : [ "-", [ { "Ref" : "Target" }, { "Ref" : "Stack" } ]]}} ]
      }
    },
    "PubSubnet1" : {
      "Type" : "AWS::EC2::Subnet",
      "Properties" : {
        "CidrBlock" : { "Ref" : "PubSubCIDR1" },
        "MapPublicIpOnLaunch" : "true",
        "Tags" : [ { "Key" : "Name", "Value" : { "Fn::Join" : [ "-", [ { "Ref" : "Target" }, { "Ref" : "Stack" }, "pub1" ]]}} ],
        "VpcId" : { "Ref" : "VPC" }
      }
    }
  },

  "Outputs" : {
  }
}















