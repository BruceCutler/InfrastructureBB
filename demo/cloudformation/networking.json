{
  "AWSTemplateFormatVersion" : "2010-09-09",

  "Description" : "Networking Stack",

  "Metadata" : {
  },

  "Parameters" : {
    "Target" : {
      "Type" : "String"
    },
    "Stack" : {
      "Type" : "String"
    },
    "CIDRBlock" : {
      "Type" : "String"
    },
    "KeyPairName" : {
      "Type" : "String"
    },
    "PubSubCIDR" : {
      "Type" : "CommaDelimitedList"
    },
    "PrivSubCIDR" : {
      "Type" : "CommaDelimitedList"
    }
  },

  "Mappings" : {
  },

  "Conditions" : {
  },

  "Resources" : {
    "AWSVpc" : {
      "Type" : "AWS::EC2::VPC",
      "Properties" : {
        "CidrBlock" : { "Ref" : "CIDRBlock" },
        "EnableDnsHostnames" : "true",
        "Tags" : [ { "Key" : "Name", "Value" : { "Fn::Join" : [ "-", [ { "Ref" : "Target" }, { "Ref" : "Stack" } ]]}} ]
      }
    }
  },

  "Outputs" : {
  }
}